{
  "name": "umbra-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "build": "graph build",
    "codegen": "graph codegen",
    "create-local": "graph create --node http://localhost:8020/ ScopeLift/umbra-subgraph",
    "deploy:local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 ScopeLift/umbra-subgraph",
    "deploy:mainnet": "yarn prepare:mainnet && yarn deploy",
    "deploy:polygon": "yarn prepare:polygon && yarn deploy",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ ScopeLift/umbra-subgraph",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare:polygon": "mustache config/polygon.json subgraph.template.yaml > subgraph.yaml",
    "prettier": "prettier --write .",
    "remove-local": "graph remove --node http://localhost:8020/ ScopeLift/umbra-subgraph"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.23.2",
    "@graphprotocol/graph-ts": "0.23.1"
  },
  "devDependencies": {
    "mustache": "^4.2.0",
    "prettier": "^2.4.1"
  }
}
